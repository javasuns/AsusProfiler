<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<HBox fx:id="headerPane" prefWidth="500.0" stylesheets="@Header.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javasuns.profiler.asus.controller.header.HeaderController">
   <styleClass>
      <String fx:value="header" />
   </styleClass>
   <children>
      <Button fx:id="btnSettings" alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#handleButtonAction" style="-fx-padding: 0 1em; -fx-graphic-text-gap: 0.5em; -fx-pref-width: 5em;">
         <graphic>
            <Region maxHeight="1.7976931348623157E308" minWidth="-Infinity" styleClass="svg_burger_menu" />
         </graphic>
      </Button>
      <Label fx:id="lblTitle" alignment="CENTER" cache="true" cacheHint="SPEED" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" text="Title" HBox.hgrow="ALWAYS" />
      <Region maxHeight="1.7976931348623157E308" minWidth="-Infinity" style="-fx-pref-width: 5em;" />
   </children>
</HBox>
